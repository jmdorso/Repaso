#include <stdio.h>
#include <stdio_ext.h>
#include <stdlib.h>
#include <string.h>
#include "utn.h"


int main(void) {
	char texto[50];

	getString(texto,"Ingrese numero","\nError!",1,50,2);
	if(esSoloNumeros(texto,"\nNo es un numero valido")==0)
	{
		printf("%s",texto);
	}
	return EXIT_SUCCESS;
}
